 catch (error) {\n      throw error\n    }\n  }\n\n  return (\n    <Fragment>\n      <TableCell\n        component=\"th\"\n        scope=\"row\"\n        onClick={() => {\n          increaseCount()\n        }}\n      >\n        <a href={link} target=\"_blank\">\n          {link}\n        </a>\n      </TableCell>\n      <TableCell value={clickCount}>{clickCount}</TableCell>\n      <TableCell>{comment}</TableCell>\n\n      <TableCell>\n        <Button\n          className=\"UpdateInfo\"\n          variant=\"contained\"\n          color=\"primary\"\n          size=\"small\"\n          onClick={() => {\n            setActiveLink({ id, link, comment })\n            history.push('/createLink')\n          }}\n        >\n          Update\n        </Button>\n        <Button\n          className=\"UpdateInfo\"\n          variant=\"contained\"\n          color=\"primary\"\n          style={{ margin: '3px' }}\n          size=\"small\"\n          onClick={async () => {\n            try {\n              let del = await deleteLink(id)\n              let result = linkList.filter((link) => link.id !== id)\n              setLinkList(result)\n            } catch (error) {\n              console.error(error)\n            }\n          }}\n        >\n          Delete\n        </Button>\n      </TableCell>\n    </Fragment>\n  )\n}\n\nexport default TableRows\n"},"lineCount":null}},"error":null,"hash":"612588c2d5a6ead676e0dccec21fc976","cacheData":{"env":{}}}