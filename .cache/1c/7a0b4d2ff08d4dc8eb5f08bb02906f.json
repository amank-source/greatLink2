;\n}\n\n.createLink {\n\tcolor: white;\n\tmargin: auto;\n\tfloat: center;\n}\n\n.createLinkButton * {\n\tmargin: auto;\n\tfloat: center;\n}\n\n.newLinkForm {\n\tdisplay: grid;\n\tgrid-template-columns: repeat(auto-fit, minmax(240px, 1fr));\n\tjustify-content: center;\n\talign-content: center;\n\tgrid-gap: 10px;\n}\n.createLink {\n\ttext-decoration: none;\n}\n\n.newLinkForm h3 {\n\tgrid-row: 1;\n\tgrid-column: 2 / 3;\n\tjustify-self: center;\n\talign-self: center;\n\tpadding-top: 2em;\n}\n\n.urlName {\n\tgrid-row: 2;\n\tgrid-column: 2;\n\tfont-weight: bold;\n}\n\n.urlPrefix {\n\tgrid-row: 3;\n\tgrid-column: 1;\n\tmargin-bottom: 1em;\n\tjustify-self: end;\n\talign-self: center;\n\tpadding: 7px;\n}\n\n.linkName {\n\tgrid-row: 3;\n\tgrid-column: 2;\n\tborder: 1px solid green;\n\tmargin-bottom: 1em;\n\tpadding: 7px 11px;\n}\n\n.urlSuffix {\n\tgrid-row: 3;\n\tgrid-column: 3;\n\tpadding: 7px;\n}\n\n.commentLabel {\n\tgrid-row: 4;\n\tgrid-column: 2;\n\tfont-weight: bold;\n}\n\n.commentBox {\n\tgrid-row: 5;\n\tgrid-column: 2 / 3;\n\tpadding: calc(0.75em - 1px);\n\tmargin-bottom: 1em;\n\tborder: 1px solid green;\n}\n\n.tagsLabel {\n\tgrid-row: 6;\n\tgrid-column: 2;\n\tfont-weight: bold;\n}\n\n.tags {\n\tgrid-row: 7;\n\tgrid-column: 2 / 3;\n\tpadding: 1em;\n\tmargin-bottom: 1em;\n\tborder: 1px solid green;\n}\n\n.LinkList-head {\n\tbackground-color: #9999ab;\n\tfont-family: monospace;\n\tcolor: white;\n}\n\n.buttonGroup {\n\tgrid-row: 8;\n\tgrid-column: 2;\n\tmax-width: 1.5em;\n\tdisplay: grid;\n\tgrid-template-columns: repeat(auto-fit, minmax(100px, 1fr));\n}\n\n.submit {\n\tmax-width: 1em;\n\tgrid-row: 1;\n\tgrid-column: 1;\n}\n\n.cancel {\n\tgrid-row: 1;\n\tgrid-column: 2;\n\tmax-width: 1.5em;\n}\n"},"lineCount":205}},"error":null,"hash":"d9d7c160c3c4d9751663dc171def8df0","cacheData":{"env":{}}}